package fundamentos_objetos;

public class Medico {
	int codigo;
	String nome;
	String genero;
	String especialidade;
	String endereco;
	
	public Medico(int codigo, String nome,String genero,String especialidade,String endereco) {
		this.codigo = codigo;
		this.nome = nome;
		this.genero = genero;
		this.especialidade = especialidade;
		this.endereco = endereco;
	}
}

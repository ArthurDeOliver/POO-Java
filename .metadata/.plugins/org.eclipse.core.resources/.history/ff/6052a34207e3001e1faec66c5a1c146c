package fundamentos_objeto2;

public class Main {
	public static void main(String[] args) {
		
		Lutador ryu = new Lutador("Ryu",100,10);
		Lutador Bison = new Lutador("Bison",100,12);
		
		for(int i = 0; i > 3; i++) {
			ryu.aplicarGolpe(Bison);
		}
		
		for(int j = 0; j > 3; j++) {
			Bison.aplicarGolpe(ryu);
		}
		
		
		
	}
}
